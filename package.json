{
  "name": "stack-editor",
  "version": "0.1.0",
  "description": "Stack editor",
  "main": "index.js",
  "scripts": {
    "tiye": "rsync -r dist/ cirru.org:repo/Cirru/stack-editor",
    "release": "shadow-cljs release release",
    "watch": "shadow-cljs watch browser",
    "compile-ssr": "shadow-cljs compile ssr",
    "html-dev": "env=dev node target/app.render.js",
    "html": "node target/app.render.js",
    "hmr": "webpack-dev-server --config webpack.dev.coffee --hot-only",
    "webpack": "webpack --config webpack.release.coffee --hide-modules --progress",
    "dev": "yarn compile && yarn html-dev && yarn hmr",
    "del": "rm -rf dist/*",
    "serve": "http-server dist",
    "build": "yarn del && yarn release && yarn webpack && yarn compile-ssr && yarn html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cirru/stack-editor"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "coffee-script": "^1.12.6",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "respo-ui": "^0.1.2",
    "shadow-cljs": "^0.14.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
